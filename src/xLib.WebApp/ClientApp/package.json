{
    "name": "xlib-client-app",
    "productName": "xlib-client-app",
    "version": "0.0.1",
    "description": "",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx ."
    },
    "author": {
        "name": "W0rlDx"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "xlib_client_app"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "devServer": {
                            "proxy": {
                                "/api": {
                                    "target": "https: //localhost:7237/api",
                                    "pathRewrite": {
                                        "^/api": ""
                                    },
                                    "secure": false,
                                    "changeOrigin": true,
                                    "logLevel": "debug"
                                },
                                "/swagger": {
                                    "target": "https: //localhost:7237/swagger",
                                    "pathRewrite": {
                                        "^/swagger": ""
                                    },
                                    "secure": false,
                                    "changeOrigin": true,
                                    "logLevel": "debug"
                                }
                            }
                        },
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/Index.html",
                                    "js": "./src/Renderer.ts",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
        "@types/dotenv-webpack": "^7.0.3",
        "@types/loadable__component": "^5.13.4",
        "@types/node": "^17.0.36",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.5",
        "@types/react-table": "^7.7.12",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@typescript-eslint/typescript-estree": "^5.27.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.0",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.7.1",
        "dotenv-webpack": "^7.1.0",
        "electron": "^19.0.1",
        "esbuild-loader": "^2.19.0",
        "eslint": "^8.16.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.4.6",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-webpack-plugin": "^3.1.1",
        "fork-ts-checker-webpack-plugin": "^7.2.11",
        "html-webpack-plugin": "^5.5.0",
        "node-loader": "^2.0.0",
        "node-sass": "^7.0.1",
        "path": "^0.12.7",
        "prettier": "^2.6.2",
        "prettier-eslint": "^15.0.1",
        "prettier-eslint-cli": "^6.0.1",
        "sass-loader": "^13.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "@loadable/component": "^5.15.2",
        "@mantine/core": "^4.2.7",
        "@mantine/dates": "^4.2.7",
        "@mantine/dropzone": "^4.2.7",
        "@mantine/hooks": "^4.2.7",
        "@mantine/modals": "^4.2.7",
        "@mantine/notifications": "^4.2.7",
        "@mantine/spotlight": "^4.2.7",
        "@mantine/styles": "^4.2.7",
        "axios": "^0.27.2",
        "dayjs": "^1.11.2",
        "electron-squirrel-startup": "^1.0.0",
        "process": "^0.11.10",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-icons": "^4.4.0",
        "react-query": "^4.0.0-beta.9",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "react-table": "^7.8.0"
    }
}